{
  "collectCoverage": true,
  "collectCoverageFrom": ["src/**/!(*.test).js", "!src/index.js"],
  "coverageDirectory": "./coverage",
  "coverageReporters": ["lcov", "text"],
  "testMatch": ["**/(*.test).js"],
  "transformIgnorePatterns": ["!node_modules/"],
  "verbose": true,
  "detectOpenHandles": true
}
